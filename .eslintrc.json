{
    "env": {
        "node": true,
        "es2022": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 2022
    },
    "rules": {
        "no-duplicate-imports": "error",
        "no-redeclare": "error",
        "no-unused-vars": [
            "warn",
            {
                "argsIgnorePattern": "^_"
            }
        ],
        "no-undef": "error"
    }
}